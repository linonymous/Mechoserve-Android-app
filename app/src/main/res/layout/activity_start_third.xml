<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.thawari.servicesprovided.IconsOnetime">

    <TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Text134"
        android:layout_marginTop="7dp"
        android:text="We Provide Services For.."
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:textSize="23dp"
        android:textStyle="bold"
        />
    <ImageView
        android:id="@+id/bajaj"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/bajaj"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="10dp"/>

    <ImageView
        android:id="@+id/hero"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/hero"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        />

    <ImageView
        android:id="@+id/suzuki"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/suzuki"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="244.5dp"
        />

    <ImageView
        android:id="@+id/mahindra"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/mahindra"
        android:layout_marginTop="179dp"
        android:layout_marginLeft="10dp"/>

    <ImageView
        android:id="@+id/tvs"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/tvs"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="179dp"
        />

    <ImageView
        android:id="@+id/piaggio"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/piaggio"
        android:layout_marginTop="179dp"
        android:layout_marginLeft="244.5dp"
        />

    <ImageView
        android:id="@+id/honda"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/honda"
        android:layout_marginTop="290dp"
        android:layout_marginLeft="52dp"
        />

    <ImageView
        android:id="@+id/yamaha"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/yamaha"
        android:layout_marginTop="290dp"
        android:layout_marginLeft="190dp"
        />

    <Button android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/startthirdnext"
        android:layout_below="@+id/yamaha"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:text="  Take Me In  "
        android:textColor="#FFFFFF"
        android:background="@drawable/temp_button"/>

</RelativeLayout>
